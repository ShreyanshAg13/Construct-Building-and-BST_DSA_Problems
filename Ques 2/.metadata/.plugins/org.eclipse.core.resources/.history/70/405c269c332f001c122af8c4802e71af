package assignmentGL.BST.Node;


public class Node {
	

	
	public static int val;
    public static Node left;
	public static Node right;
	
	public static Node node;
    public static Node prevNode = null;
    public static Node headNode = null;
	
    
	public Node(int i) {
		val = i;
        left = right = null;
	}


	public Node() {
		
	}


	

}
